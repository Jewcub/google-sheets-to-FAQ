{
  "name": "server",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "yarn build && env NODE_ENV='production' node dist/index.js",
    "dev": "ts-node-dev --inspect --poll --respawn ./src src/index.ts",
    "build": "rm -rf dist && tsc -p tsconfig.build.json && mkdir ./dist/files && cp ./src/files/token.json ./dist/files/token.json",
    "test": "jest",
    "test-watch": "jest --watchAll"
  },
  "dependencies": {
    "cheerio": "^1.0.0-rc.5",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "~4.16.1",
    "googleapis": "^67.0.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.11",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.31",
    "@typescript-eslint/eslint-plugin": "^4.15.2",
    "@typescript-eslint/parser": "^4.15.2",
    "eslint": "^7.19.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "ts-jest": "^26.5.2",
    "ts-node-dev": "^1.1.6",
    "typescript": "^4.2.2"
  }
}